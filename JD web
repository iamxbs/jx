        <script src="jquery-3.1.0.js"></script>
        <script>
            $(function(){
                $("#sendto").hover(function(){
                    $(this).css({
                        "background-color":"white",
                        "border":"1px solid #E5E5E5",
                        "border-bottom":"1px solid white",
                        "border-top":"1px solid white"
                    });
                    $(this).children().first().css({
                            "transform":"rotateZ(180deg)"
                    });
                    $(this).children().last().css({
                        "background-color":"white"
                    });
                    $(this).children().last().fadeIn("fast");
                    },function(){
                            $(this).css({
                                "background-color":"#f1f1f1",
                                "border":"0"
                        });
                            $(this).children().first().css({
                            "transform":"rotateZ(0deg)"
                        });
                            $(this).children().last().hide("fast");
                    });
                $("body").click(function(){
                    $("#tl").hide(); 
                    $("#sendto").css({
                        "background-color":"#f1f1f1",
                        "border":"0",
                    });
                });
                $("#myjd").hover(function(){
                        $(this).css({
                                "background-color":"white",
                                "border":"1px solid #E5E5E5",
                                "border-bottom":"1px solid white",
                                "border-top":"1px solid white"
                        });
                        $(this).children().last().fadeIn("fast");
                },function(){
                        $(this).css({
                                "background-color":"transparent",
                                "border":"0px;"
                        });
                        $(this).children().last().hide("fast");
                })
                    $("#shopcar").hover(function(){
                        $("#shopcar").css({
                        "background-color":"white",
                        "border-buttom":"0",               
                    });
                    $(".things").fadeIn();
                    },function(){
                         $("#shopcar").css({
                        "background-color":"#F5F5F5",
                        "border-buttom":"1px solid #E5E5E5",
                    });
                         $(".things").hide();
                    });


                    $(".sl").hover(function(){
                        $(this).css({
                            "background-color":"white",
                            "cursor":"pointer",
                            // "border-right":"none"
                        });
                        $("#tl").hide(); 
                        $("#sendto").css({
                        "background-color":"#f1f1f1",
                        "border":"0",
                    });
                        $(this).children().first().css({
                            "color":"black"
                        });
                        $(this).children().children().first().hide();
                        $(this).children().last().fadeIn("fast");
                    },function(){
                        $(this).css({
                            "background-color":"#b61d1d",
                            "cursor":"normal"
                        });
                        $(this).children().first().css({
                            "color":"white"
                        });
                        $(this).children().children().first().fadeIn("fast");
                        $(this).children().last().hide();
                    });
                    $(".sr").click(function(){
                            $(this).css({
                                    "display":"none"
                            });
                    });
                    $(".sdiv").hover(function(){
                            $(".buytic").css({
                                    "display":"inline-block",
                                //     "z-index":"4000000"
                            });
                            $(".buytic").animate({"top":"165px"},"normal","linear");
                    },function(){
                        //     $(".buytic").animate({"top":"382px"},"normal","linear");
                        //      $(".buytic").css({
                        //             "display":"none"
                    });
                    $(".buytic").click(function(){
                            $(".buytic").animate({"top":"382px"},"normal","linear");
                    });
                    $(".tb").hover(function(){
                    $(this).css({
                     "background-color":"rgba(3,168,158,0.4)",
                     "cursor":"pointer"
                    });
                    },function(){
                    $(this).css({
                     "background-color":"rgba(255,255,255,0)"
                    });
               });
             $("#A1").click(function(){
                 $("body").animate({scrollTop:'1010px'},200);
             });
             $("#B2").click(function(){
                 $("body").animate({scrollTop:'1560px'},200);
             });
             $("#C3").click(function(){
                 $("body").animate({scrollTop:'2110px'},200);
             });
             $("#D4").click(function(){
                 $("body").animate({scrollTop:'2710px'},200);
             });
             $("#E5").click(function(){
                 $("body").animate({scrollTop:'3310px'},200);
             });
             $("#F6").click(function(){
                 $("body").animate({scrollTop:'3910px'},200);
             });
             $("#H7").click(function(){
                 $("body").scrollTop(0);
                 $("div:not(#H)").css({
                      "background-color":"rgba(255,255,255,0)"
                 });
             });
             $(window).scroll(function(){
                 var top = $("body").scrollTop();
       
             if(top>=0 && top<810){
             $(".tb").css({
                     "display":"none"
             });
                  $("#A1").css({
                      "background-color":"rgba(255,255,255,0)"
                 });
             }else{
                     $(".tb").css({
                     "display":"inline-block"
             });
             }
             if(top>=1010 && top<1560){
                 $("#A1").css({
                      "background-color":"rgba(3,168,158,0.4)"
                 });
                 $("div:not(#A1)").css({
                      "background-color":"rgba(255,255,255,0)"
                 });
             }else 
             if(top>=1560 && top<2110){
                 $("#B2").css({
                      "background-color":"rgba(3,168,158,0.4)"
                 });
                 $("div:not(#B2)").css({
                      "background-color":"rgba(255,255,255,0)"
                 });
             }else 
             if(top>=2110 && top<2710){
                 $("#C3").css({
                      "background-color":"rgba(3,168,158,0.4)"
                 });
                 $("div:not(#C3)").css({
                      "background-color":"rgba(255,255,255,0)"
                 });
             }else 
             if(top>=2710 && top<3310){
                 $("#D4").css({
                      "background-color":"rgba(3,168,158,0.4)"
                 });
                 $("div:not(#D4)").css({
                      "background-color":"rgba(255,255,255,0)"
                 });
             }else 
             if(top>=3310 && top<3910){
                 $("#E5").css({
                      "background-color":"rgba(3,168,158,0.4)"
                 });
                 $("div:not(#E5)").css({
                      "background-color":"rgba(255,255,255,0)"
                 });
             }else 
             if(top>=3910 && top<4227){
                 $("#F6").css({
                      "background-color":"rgba(3,168,158,0.4)"
                 });
                 $("div:not(#F6)").css({
                      "background-color":"rgba(255,255,255,0)"
                 });
             };
             });
            });
            </script>
        <script>
            $(function(){
            var cs = document.getElementsByClassName("c");
            var c1 = document.getElementsByClassName("c1_");
            var c2 = document.getElementsByClassName("c2_");
            var c3 = document.getElementsByClassName("c3_");
            var c4 = document.getElementsByClassName("c4_");
            var c5 = document.getElementsByClassName("c5_");
            var c6 = document.getElementsByClassName("c6_");
            var c7 = document.getElementsByClassName("c7_");
            var imgs = document.getElementById("imgs");
            var imgarr = ["mz.jpg","919.jpg","iphone.jpg","ljm.jpg","hp.jpg","computer.jpg","buy.jpg"];
            var index = 0;
            var key;
            var move;
            function move(){
             index++;
             if(index == 6){
                 index = 0;
             }
             imgs.style.backgroundImage = "url('"+imgarr[index]+"')";
             Array.prototype.forEach.call(cs,function(x,i){
                if(i == index){
                    x.style.backgroundColor = "red";
                }else{
                    x.style.backgroundColor = "#595959";
                }
            });
            };
             key = setInterval(move,2000);
             $(".c1_").click(function(){
                     clearInterval(key);
                     $("#imgs").css({
                             "background-image":"url('mz.jpg')"
                     });
                     $(this).css({
                             "background-color":"red"
                     });
                     $(".c:not(_c1)").css({
                             "background-color":"#595959"
                     });
                     key = setInterval(move,2000);
             });
             $(".c2_").click(function(){
                     clearInterval(key);
                     $("#imgs").css({
                             "background-image":"url('919.jpg')"
                     });
                     $(this).css({
                             "background-color":"red"
                     });
                     $(".c:not(_c1)").css({
                             "background-color":"#595959"
                     });
                     key = setInterval(move,2000);
             });
             $(".c3_").click(function(){
                     clearInterval(key);
                     $("#imgs").css({
                             "background-image":"url('iphone.jpg')"
                     });
                     $(this).css({
                             "background-color":"red"
                     });
                     $(".c:not(_c1)").css({
                             "background-color":"#595959"
                     });
                     key = setInterval(move,2000);
             });
             $(".c4_").click(function(){
                     clearInterval(key);
                     $("#imgs").css({
                             "background-image":"url('ljm.jpg')"
                     });
                     $(this).css({
                             "background-color":"red"
                     });
                     $(".c:not(_c1)").css({
                             "background-color":"#595959"
                     });
                     key = setInterval(move,2000);
             });
             $(".c5_").click(function(){
                     clearInterval(key);
                     $("#imgs").css({
                             "background-image":"url('hp.jpg')"
                     });
                     $(this).css({
                             "background-color":"red"
                     });
                     $(".c:not(_c1)").css({
                             "background-color":"#595959"
                     });
                     key = setInterval(move,2000);
             });
             $(".c6_").click(function(){
